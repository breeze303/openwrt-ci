# -------------------------
# 内核 Netfilter（关键：防止 Kconfig 交互）
# -------------------------
CONFIG_KERNEL_NETFILTER=y
CONFIG_KERNEL_NETFILTER_ADVANCED=y

CONFIG_KERNEL_NETFILTER_INGRESS=y
CONFIG_KERNEL_NETFILTER_EGRESS=y

CONFIG_KERNEL_NETFILTER_NETLINK=m
CONFIG_KERNEL_NETFILTER_NETLINK_HOOK=n
CONFIG_KERNEL_NETFILTER_NETLINK_ACCT=n
CONFIG_KERNEL_NETFILTER_NETLINK_QUEUE=n
CONFIG_KERNEL_NETFILTER_NETLINK_LOG=n
CONFIG_KERNEL_NETFILTER_NETLINK_OSF=n

CONFIG_KERNEL_NF_CONNTRACK=m
CONFIG_KERNEL_NF_LOG_SYSLOG=m
CONFIG_KERNEL_NF_CONNTRACK_MARK=y
CONFIG_KERNEL_NF_CONNTRACK_ZONES=y
CONFIG_KERNEL_NF_CONNTRACK_PROCFS=y
CONFIG_KERNEL_NF_CONNTRACK_EVENTS=y
CONFIG_KERNEL_NF_CONNTRACK_TIMEOUT=y

# conntrack（必须启用）
CONFIG_PACKAGE_kmod-ipt-conntrack=y

# -------------------------
# 加速插件
# -------------------------
CONFIG_PACKAGE_kmod-tcp-bbr=y
CONFIG_PACKAGE_kmod-ipt-offload=y

# IRQ 多核均衡
CONFIG_PACKAGE_irqbalance=y

# FQ-CoDel
CONFIG_PACKAGE_kmod-sched-core=y
CONFIG_PACKAGE_kmod-sched-fq-codel=y


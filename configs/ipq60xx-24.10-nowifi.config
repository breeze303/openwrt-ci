CONFIG_TARGET_ipq60xx=y
CONFIG_TARGET_ipq60xx_generic=y
CONFIG_TARGET_ipq60xx_generic_DEVICE_zte_m2=y

# 内核版本（LibWrt 固定 6.12）
CONFIG_LINUX_6_12=y

# ===========================
# 关闭 WiFi（NOWIFI）
# ===========================
CONFIG_PACKAGE_kmod-ath11k=n
CONFIG_PACKAGE_kmod-ath11k-pci=n
CONFIG_ATH11K=n
CONFIG_ATH11K_PCI=n
CONFIG_PACKAGE_ath11k-firmware-ipq6018=n
CONFIG_PACKAGE_ath11k-firmware-ipq8074=n
CONFIG_PACKAGE_ath11k-firmware-qcn9074=n
CONFIG_PACKAGE_ath11k-firmware-qcn6122=n
CONFIG_PACKAGE_ath11k-firmware-qcn9160=n
CONFIG_PACKAGE_wireless-regdb=n
CONFIG_PACKAGE_wpad-basic=n
CONFIG_PACKAGE_wpad-openssl=n
CONFIG_PACKAGE_hostapd=n
CONFIG_PACKAGE_iw=n
CONFIG_PACKAGE_iwinfo=n
CONFIG_PACKAGE_kmod-cfg80211=n
CONFIG_PACKAGE_kmod-mac80211=n

# ===========================
# NSS 加速（LibWrt 完整支持）
# ===========================
CONFIG_PACKAGE_kmod-qca-nss-dp=y
CONFIG_PACKAGE_kmod-qca-nss-ecm=y
CONFIG_PACKAGE_kmod-qca-nss-gmac=y
CONFIG_PACKAGE_kmod-qca-nss-crypto=y
CONFIG_PACKAGE_kmod-qca-nss-l2tp=y
CONFIG_PACKAGE_kmod-qca-nss-tun6rd=y
CONFIG_PACKAGE_kmod-qca-nss-ppe=y
CONFIG_PACKAGE_kmod-qca-nss-macsec=y
CONFIG_PACKAGE_kmod-qca-nss-macvlan=y
CONFIG_PACKAGE_qca-nss-dp=y
CONFIG_PACKAGE_qca-nss-ecm=y

# SSDK（交换机驱动）
CONFIG_PACKAGE_kmod-qca-ssdk=y
CONFIG_PACKAGE_qca-ssdk-shell=y

# ===========================
# TurboACC（NSS + SFE）
# ===========================
CONFIG_PACKAGE_luci-app-turboacc=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_SHORTCUT_FE=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_BBR_CCA=y
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_DNSFORWARDER=n
CONFIG_PACKAGE_luci-app-turboacc_INCLUDE_DNSPROXY=n

# ===========================
# 基础系统
# ===========================
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-compat=y
CONFIG_PACKAGE_luci-base=y
CONFIG_PACKAGE_luci-mod-admin-full=y
CONFIG_PACKAGE_luci-theme-argon=y
CONFIG_PACKAGE_luci-theme-design=y

CONFIG_PACKAGE_curl=y
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_nano=y
CONFIG_PACKAGE_vim-full=y
CONFIG_PACKAGE_unzip=y
CONFIG_PACKAGE_tar=y

# ===========================
# 防火墙（nft）
# ===========================
CONFIG_PACKAGE_firewall4=y
CONFIG_PACKAGE_kmod-nft-offload=y

# ===========================
# Passwall2
# ===========================
CONFIG_PACKAGE_luci-app-passwall2=y
CONFIG_PACKAGE_passwall2=y
CONFIG_PACKAGE_passwall2-core=y
CONFIG_PACKAGE_passwall2-proxy=y
CONFIG_PACKAGE_passwall2-libs=y

# ===========================
# OpenClash
# ===========================
CONFIG_PACKAGE_luci-app-openclash=y

# ===========================
